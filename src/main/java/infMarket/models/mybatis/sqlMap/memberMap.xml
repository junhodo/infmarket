        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="infMarket.models.mybatis.dao.MemberMapper">
 <resultMap type="infMarket.models.mybatis.dto.Member" id="memberMap">
  <id column="id" property="id"/>
  <result column="pw" property="pw"/>
 </resultMap>
 <select id="selectAllMembers" resultMap="memberMap">
  select id as id,
   pw as pw
  from Member;
 </select>

